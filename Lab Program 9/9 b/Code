#include <stdio.h>
#include <stdlib.h>

#define MAX 10

void DFS(int graph[MAX][MAX], int node, int visited[MAX], int n) {
    visited[node] = 1;
    printf("%d ", node);
    for (int i = 0; i < n; i++) {
        if (graph[node][i] == 1 && !visited[i]) {
            DFS(graph, i, visited, n);
        }
    }
}

int isConnected(int graph[MAX][MAX], int n) {
    int visited[MAX] = {0};
    DFS(graph, 0, visited, n);
    for (int i = 0; i < n; i++) {
        if (!visited[i]) {
            return 0;
        }
    }
    return 1;
}

int main() {
    int n;
    printf("Number of vertices : ");
    scanf("%d", &n);
    
    int graph[MAX][MAX] = {0};
    printf("Adjacency matrix : ");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &graph[i][j]);
        }
    }
    
    if (isConnected(graph, n)) {
        printf("\nConnected");
    } else {
        printf("\nNot Connected");
    }
    return 0;
}
